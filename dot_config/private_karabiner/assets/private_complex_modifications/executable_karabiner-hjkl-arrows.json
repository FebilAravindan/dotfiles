{
    "title": "Right Command + HJKL Navigation & Deletion (No Conflicts)",
    "rules": [
        {
            "description": "Right Command + HJKL to Arrow Keys (with modifier passthrough)",
            "manipulators": [
                {
                    "description": "Right Cmd + H = Left Arrow",
                    "type": "basic",
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "left_arrow" }]
                },
                {
                    "description": "Right Cmd + J = Down Arrow",
                    "type": "basic",
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "down_arrow" }]
                },
                {
                    "description": "Right Cmd + K = Up Arrow",
                    "type": "basic",
                    "from": {
                        "key_code": "k",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "up_arrow" }]
                },
                {
                    "description": "Right Cmd + L = Right Arrow",
                    "type": "basic",
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "right_arrow" }]
                }
            ]
        },
        {
            "description": "Right Command + UIOP for Page Navigation",
            "manipulators": [
                {
                    "description": "Right Cmd + U = Page Up",
                    "type": "basic",
                    "from": {
                        "key_code": "u",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "page_up" }]
                },
                {
                    "description": "Right Cmd + I = Home",
                    "type": "basic",
                    "from": {
                        "key_code": "i",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "home" }]
                },
                {
                    "description": "Right Cmd + O = End",
                    "type": "basic",
                    "from": {
                        "key_code": "o",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "end" }]
                },
                {
                    "description": "Right Cmd + P = Page Down",
                    "type": "basic",
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": ["right_command"],
                            "optional": ["shift", "option", "control"]
                        }
                    },
                    "to": [{ "key_code": "page_down" }]
                }
            ]
        },
        {
            "description": "Right Command + NM,. for Deletion Keys",
            "manipulators": [
                {
                    "description": "Right Cmd + N = Delete Word Backward",
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": ["right_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_or_backspace", "modifiers": ["option"] }]
                },
                {
                    "description": "Right Cmd + Left Cmd + N = Delete to Line Start",
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": ["right_command", "left_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_or_backspace", "modifiers": ["command"] }]
                },
                {
                    "description": "Right Cmd + Option + N = Delete Entire Line",
                    "type": "basic",
                    "from": {
                        "key_code": "n",
                        "modifiers": {
                            "mandatory": ["right_command", "option"]
                        }
                    },
                    "to": [
                        { "key_code": "a", "modifiers": ["control"] },
                        { "key_code": "k", "modifiers": ["control"] }
                    ]
                },
                {
                    "description": "Right Cmd + M = Forward Delete (char)",
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": ["right_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_forward" }]
                },
                {
                    "description": "Right Cmd + Left Cmd + M = Delete Word Forward",
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": ["right_command", "left_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_forward", "modifiers": ["option"] }]
                },
                {
                    "description": "Right Cmd + Option + M = Move Line Down",
                    "type": "basic",
                    "from": {
                        "key_code": "m",
                        "modifiers": {
                            "mandatory": ["right_command", "option"]
                        }
                    },
                    "to": [{ "key_code": "down_arrow", "modifiers": ["option"] }]
                },
                {
                    "description": "Right Cmd + Comma = Backspace (char)",
                    "type": "basic",
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": ["right_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_or_backspace" }]
                },
                {
                    "description": "Right Cmd + Left Cmd + Comma = Delete Word Backward",
                    "type": "basic",
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": ["right_command", "left_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_or_backspace", "modifiers": ["option"] }]
                },
                {
                    "description": "Right Cmd + Option + Comma = Move Line Up",
                    "type": "basic",
                    "from": {
                        "key_code": "comma",
                        "modifiers": {
                            "mandatory": ["right_command", "option"]
                        }
                    },
                    "to": [{ "key_code": "up_arrow", "modifiers": ["option"] }]
                },
                {
                    "description": "Right Cmd + Period = Delete Word Forward",
                    "type": "basic",
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "mandatory": ["right_command"]
                        }
                    },
                    "to": [{ "key_code": "delete_forward", "modifiers": ["option"] }]
                },
                {
                    "description": "Right Cmd + Left Cmd + Period = Delete to Line End",
                    "type": "basic",
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "mandatory": ["right_command", "left_command"]
                        }
                    },
                    "to": [{ "key_code": "k", "modifiers": ["control"] }]
                },
                {
                    "description": "Right Cmd + Option + Period = Delete Entire Line",
                    "type": "basic",
                    "from": {
                        "key_code": "period",
                        "modifiers": {
                            "mandatory": ["right_command", "option"]
                        }
                    },
                    "to": [
                        { "key_code": "a", "modifiers": ["control"] },
                        { "key_code": "k", "modifiers": ["control"] }
                    ]
                }
            ]
        },
        {
            "description": "Left Control + J = Enter",
            "manipulators": [
                {
                    "description": "Left Ctrl + J = Return/Enter",
                    "type": "basic",
                    "from": {
                        "key_code": "j",
                        "modifiers": {
                            "mandatory": ["left_control"]
                        }
                    },
                    "to": [{ "key_code": "return_or_enter" }]
                }
            ]
        },
        {
            "description": "CapsLock to Escape + Left Control (dual function)",
            "manipulators": [
                {
                    "description": "CapsLock alone = Escape, held = Left Control",
                    "type": "basic",
                    "from": {
                        "key_code": "caps_lock",
                        "modifiers": { "optional": ["any"] }
                    },
                    "to": [{ "key_code": "left_control" }],
                    "to_if_alone": [{ "key_code": "escape" }]
                }
            ]
        }
    ]
}
